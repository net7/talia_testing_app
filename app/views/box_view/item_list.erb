
  <div class="box_index_node">
<ul>
<% @elements.each do |e| %>
      <li>
<!--        <div class="box_index_node">-->
          <div class="box_index_leaf">
          <h3>
            <a class="resource" href="/boxView/dispatch?method=getSource&resource=<%= Base64.encode64s(e.to_s) %>&type=source"><%= e.uri.to_uri.local_name.gsub('_', ' ') %> </a>
          </h3>
        </div>
      </li>
    <% end %>
</ul>
  </div>

